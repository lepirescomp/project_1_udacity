    "# percentage of man and women\n",
    "quantity_of_man_women = dict()\n",
    "for dataset, year in zip(datasets_format1, [2018,2019,2020]):\n",
    "    man_woman = dataset[[\"Respondent\", \"Gender\"]][\"Gender\"].value_counts().iloc[:2]\n",
    "    total = dataset.shape[0]\n",
    "    if \"Female\" in man_woman:     \n",
    "        quantity_of_man_women[year] = {\"Woman\": man_woman[\"Female\"]/total, \"Man\": man_woman[\"Male\"]/total}\n",
    "    if \"Woman\" in man_woman:\n",
    "        quantity_of_man_women[year] = {\"Woman\": man_woman[\"Woman\"]/total, \"Man\": man_woman[\"Man\"]/total}\n",
    "\n",
    "    # quantity_of_man_women.append(, dataset.shape[0]))\n"
   ]
  },
